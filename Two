import pyautogui
import time
pauseseconds=2000000

time.sleep(2)
#MULTIPLICATION
#Click button 7
pyautogui.moveTo(1292,192,_pause=pauseseconds)
pyautogui.click(_pause=pauseseconds)
#Click button 8
pyautogui.moveTo(1390,201,_pause=pauseseconds)
pyautogui.click(_pause=pauseseconds)
#Click button *
pyautogui.moveTo(1582,196,_pause=pauseseconds)
pyautogui.click(_pause=pauseseconds)
#Click button 5
